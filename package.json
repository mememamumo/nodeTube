{
  "name": "nodetube",
  "version": "1.0.0",
  "description": "Create website with NodeJs and VanillaJs",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static'",
    "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "cp -R src/static build && npm run build:assets && cp -R src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rm -rf build",
    "tunnel": "ngrok http 4000",
    "start": "PRODUCTION=true node build/init.js",
    "prestart": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mememamumo/nodeTube.git"
  },
  "author": "Yuna Lee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mememamumo/nodeTube/issues"
  },
  "homepage": "https://github.com/mememamumo/nodeTube#readme",
  "dependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "autoprefixer": "^9.8.0",
    "aws-sdk": "^2.702.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.0.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "get-blob-duration": "^1.1.1",
    "helmet": "^3.22.0",
    "moment": "^2.26.0",
    "mongoose": "^5.9.15",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "ngrok": "^3.2.7",

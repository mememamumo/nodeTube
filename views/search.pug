extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3.titb.tlt Searching for - 
            span #{searchingBy}
    .search__videos
        if videos.length === 0
            h4.txtb.lt 비디오를 찾을 수 없습니다.
        each item in videos
            +videoBlock({
                id:item.id,
                title:item.title,
                views:item.views,
                videoFile:item.fileUrl
            })